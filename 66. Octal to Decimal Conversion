#include<stdio.h>
#include<math.h>
int convert(long long num)
{
    int i = 0, decimal = 0;
    int base = 8;
    while (num!=0)
    {
        int digit = num % 10;
        decimal += digit * pow(base, i);

        num /= 10;
        i++;
    }
    return decimal;
}
int main()
{
    long long octal;
    
    printf("Enter Octal Number: ");
    scanf("%lld", &octal);
    
    printf("Decimal: %lld", convert(octal));
    
    return 0;
}
